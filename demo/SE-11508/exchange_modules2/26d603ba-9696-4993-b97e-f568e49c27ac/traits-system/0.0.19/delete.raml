#%RAML 1.0 Trait

responses:
  204:
    description: No Content
    headers: 
      X-Trace-Id:
        required: false
      X-Request-Id:
        required:  true
      X-Message-Id:
        required:  true      
  400:
    description: Bad request.
    headers: 
      X-Trace-Id:
        required: false
      X-Request-Id:
        required:  true
      X-Message-Id:
        required:  true
  404:
    description: Resource not found.
    headers: 
      X-Trace-Id:
        required: false
      X-Request-Id:
        required:  true
      X-Message-Id:
        required:  true
  422:
    description: Unprocessable Entity.
    headers: 
      X-Trace-Id:
        required: false
      X-Request-Id:
        required:  true
      X-Message-Id:
        required:  true
  500:
    description: Internal Server Error.  
    headers: 
      X-Trace-Id:
        required: false
      X-Request-Id:
        required:  true
      X-Message-Id:
        required:  true